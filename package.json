{
  "name": "eumelbot",
  "version": "1.0.0",
  "description": "Discord Bot for the Eumelcord Server",
  "main": "Bot.js",
  "scripts": {
    "start": "ts-node src/Bot.ts",
    "build": "tsc -p .",
    "deploy": "ts-node src/deployCommands.ts",
    "startJS": "node dist/Bot.js",
    "deployJS": "node dist/deployCommands.js"
  },
  "bin": "dist/Bot.js",
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": "database/**/*",
    "targets": [ "node18-win-x64", "node18-linux-x64" ],
    "outputPath": "executables"
  },
  "author": "Sephi",
  "license": "ISC",
  "dependencies": {
    "@keyv/sqlite": "^3.6.4",
    "@types/cron": "^2.0.0",
    "cron": "^2.2.0",
    "discord.js": "^14.7.1",
    "keyv": "^4.5.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
